{
	"startCommand": "yarn start:test",
	"url": "http://localhost:8080",
  "targets": [
    "chromium"
  ],
  "viewport": {
    "width": null,
    "height": null
  },
  "titleBasedScreenshots": true,
  "parallelTests": 4,
  "defaultTimeout": 25000,
  "tolerance": 2.5,
  "antialiasingTolerance": 3.5,
  "pageErrorIgnore": [
    "net::ERR_ABORTED",
    "NS_BINDING_ABORTED",
    "access control checks",
    "Load request cancelled"
  ],
  "coverageExclude": [
    "/node_modules/**",
    "**/*.json5"
  ]
}
