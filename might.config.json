{
	"startCommand": "yarn start:test",
	"url": "http://localhost:8080",
  "targets": [
    "chromium"
  ],
  "viewport": {
    "width": null,
    "height": null
  },
  "titleBasedScreenshots": true,
  "parallelTests": 6,
  "defaultTimeout": 25000,
  "tolerance": 2.5,
  "antialiasingTolerance": 3.5,
  "pageErrorIgnore": [
    "play() failed because the user didn't interact with the document first",
    "net::ERR_ABORTED",
    "NS_BINDING_ABORTED",
    "access control checks",
    "Load request cancelled"
  ],
  "coverageExclude": [
    "/node_modules/**",
    "/webpack/**",
    "/ws '('ignored')'",
    "/'('webpack')'/**",
    "/'('webpack')'-dev-server/**",
    "**/*.jsonc"
  ]
}
